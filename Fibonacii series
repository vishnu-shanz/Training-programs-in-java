import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner input =new Scanner(System.in);
int n=input.nextInt();
int a=0;
int b=1;
int fib=0;
System.out.print(a + "");
System.out.print(b + "");
for (int i=2;i<=n;i++){
    fib=a+b;
    System.out.print(fib +"");
    a=b;
    b=fib;
}
